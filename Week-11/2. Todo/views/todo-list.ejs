<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>todo-list</title>

</head>
<body>

    <h2>Todo List</h2>
    <button class="new">Add Todo</button>

    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Deadline</th>
                <th>Completed</th>
            </tr>
        </thead>
        <tbody>
            <%data.forEach((item) => {%>
                <tr>
                    <td><%=item.id%></td>
                    <td><%=item.title%></td>
                    <td><%=item.dl%></td>
                    <td><%=item.completed%></td>
                </tr>
            <% }); %>
        </tbody>
    </table>

</body>
</html>